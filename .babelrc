{
	"presets": [
		[
			"next/babel",
			{
				"styled-jsx": {
					"plugins": ["styled-jsx-plugin-sass"]
				}
			}
		]
	],
	"plugins": [
		[
			"import",
			{
				"libraryName": "antd",
				"libraryDirectory": "es",
				"style": "index.css"
			}
		],
		[
			"import",
			{
				"libraryName": "@ant-design/icons",
				"libraryDirectory": "es/icons",
				"camel2DashComponentName": false
			},
			"@ant-design/icons"
		]
	]
}
